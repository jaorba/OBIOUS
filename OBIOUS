	SpeedValue += 50
end)
ESP.MouseButton1Click:Connect(function()
	esp = not esp
	if esp == true then
		ESP.BackgroundColor3 = Color3.fromRGB(51, 111, 11)
		for i,v in pairs(workspace:GetChildren()) do
			if v:IsA("Model") then
				if v:FindFirstChild("Humanoid") then
					local BBG = Instance.new("BillboardGui")
					local ESPTEXT = Instance.new("TextLabel")
					ESPTEXT.Text = v.Name.." ("..game.Players:GetPlayerFromCharacter(v).DisplayName..")"
					BBG.Parent = v.UpperTorso
					BBG.AlwaysOnTop = true
					ESPTEXT.Parent = BBG
					BBG.Size = UDim2.new(0, 200,0, 50)
					BBG.Name = "esp"
					ESPTEXT.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					ESPTEXT.BackgroundTransparency = 1.000
					ESPTEXT.Size = UDim2.new(0, 200, 0, 50)
					ESPTEXT.Font = Enum.Font.SourceSans
					ESPTEXT.TextColor3 = Color3.fromRGB(255, 255, 255)
					ESPTEXT.TextSize = 20.000
					ESPTEXT.TextStrokeTransparency = 0.000
				end
			end
		end
	else
	ESP.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
		for i,v in pairs(workspace:GetChildren()) do
			if v:IsA("Model") then
				if v:FindFirstChild("Humanoid") then
					if v.UpperTorso:FindFirstChild("esp") then
						v.UpperTorso:FindFirstChild("esp"):Destroy()
					end
				end
			end
		end
	end
end)
UIS.InputBegan:Connect(function(inp)
	if inp.KeyCode == Enum.KeyCode.C then
		if speed.Value == true then
			_G.Speed = true
			repeat wait()
				game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = SpeedValue
			until _G.Speed == false
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
		end
	end
end)
UIS.InputEnded:Connect(function(inp)
	if inp.KeyCode == Enum.KeyCode.C then
		_G.Speed = false
	end
end)
local selected = false
TextButton.MouseButton1Click:Connect(function()
	local Yes = {Position = UDim2.new(-0.017, 0,0, 0)}
	local No = {Position = UDim2.new(0.456, 0,0, 0),}
	local Yes2 = {BackgroundColor3 = Color3.fromRGB(48, 218, 18)}
	local No2 = {BackgroundColor3 = Color3.fromRGB(132, 132, 132)}
	selected = not selected
	if selected == true then
		TweenService:Create(TextButton, info, Yes):Play()
		TweenService:Create(TextButton.Parent, info, Yes2):Play()
		S1.CommandsEnabled.Value = true
	else
		TweenService:Create(TextButton, info, No):Play()
		TweenService:Create(TextButton.Parent, info, No2):Play()
		S1.CommandsEnabled.Value = false
	end
end)
