--findplayer
game:GetService("UserInputService").TextBoxFocusReleased:Connect(function(text)
	local function GetPlayer(input) 
		for _, plr in pairs(game.Players:GetPlayers()) do
			if (string.lower(input) == string.sub(string.lower(plr.Name), 1, #input)) then
				return plr;
			end
		end
	end
	if text == Pname then
		local text_1 = Pname.Text
		local plr = GetPlayer(text_1)
		if plr then
			local PINFO = Pinfo
			PINFO.User.Text = plr.Name
			PINFO.Display.Text = plr.DisplayName
			PINFO.ImageLabel.Image = game.Players:GetUserThumbnailAsync(plr.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
			wait()
			PINFO.Visible = true
		else
			script.Parent.Parent.Pinfo.Visible = false
		end
	end
end)
Teleport.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace:FindFirstChild(User.Text).HumanoidRootPart.CFrame
end)
Spectate.MouseButton1Click:Connect(function()
	workspace.CurrentCamera.CameraSubject = workspace:FindFirstChild(User.Text).Humanoid
end)
Stop.MouseButton1Click:Connect(function()
	_G.Attacking = false
end)
Katana.MouseButton1Click:Connect(function()
	_G.Attacking = true
	repeat wait()
		workspace:FindFirstChild(User.Text).HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-.5,-2.5)
	until _G.Attacking == false or workspace:FindFirstChild(User.Text).Humanoid.Health < .5
end)
SpamTeleport.MouseButton1Click:Connect(function()
	_G.Attacking = true
	repeat wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace:FindFirstChild(User.Text).HumanoidRootPart.CFrame 
	until _G.Attacking == false or workspace:FindFirstChild(User.Text).Humanoid.Health < .5
end)
workspace.ChildAdded:Connect(function(child)
	if cashaura.Value == false then return end
	if child.Name == "Money" then
		fireclickdetector(child.ClickDetector)
	end
end)
CashAura.MouseButton1Click:Connect(function()
	cashaura.Value = true
end)
prefix.Changed:Connect(function()
	cmds_2.Text = prefix.Value.."stop - Stops everything."
	cmds_3.Text = prefix.Value.."start [Player] - Spam teleports you to the selected player."
	cmds_4.Text = prefix.Value.."katana [Player] - Spam brings the selected player to you so you can spam katana on them. (Does not work with guns) (They cant see that you are bringing them)"
	cmds_5.Text = prefix.Value.."to [Player] - Teleports you to a player."
	cmds_6.Text = prefix.Value.."prefix [String] - Changes the prefix for the commands."
end)
--COMMANDS
game.Players.LocalPlayer.Chatted:Connect(function(m1)
	if S1.CommandsEnabled.Value == false then return end
	if not  workspace:FindFirstChild(game.Players.LocalPlayer.Name) then return end
	if not  workspace:FindFirstChild(game.Players.LocalPlayer.Name):FindFirstChild("HumanoidRootPart") then return end
	local HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
	local msg = string.split(m1, " ")
	if msg[1] == prefix.Value.."stop" then
		_G.Attacking = false
		_G.Speeding = false
	elseif msg[1] == prefix.Value.."start" then
		_G.Attacking = true
		local char = GetPlayer(msg[2]).Character
		repeat wait(.1)
			HRP.CFrame = char.HumanoidRootPart.CFrame
		until _G.Attacking == false or char.Humanoid.Health < .5
	elseif msg[1] == prefix.Value.."katana" then
		_G.Attacking = true
		local char = GetPlayer(msg[2]).Character
		repeat wait()
			char.HumanoidRootPart.CFrame = HRP.CFrame * CFrame.new(0,-.5,-2.5)
		until _G.Attacking == false or char.Humanoid.Health < .5
	elseif msg[1] == prefix.Value.."to" then
		local char = GetPlayer(msg[2]).Character
		HRP.CFrame = char.HumanoidRootPart.CFrame * CFrame.new(0,-.5,-2.5)
	elseif msg[1] == prefix.Value.."prefix" then
		prefix.Value = msg[2]
	end
end)
