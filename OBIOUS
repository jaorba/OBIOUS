local info = TweenInfo.new(
	.2,
	Enum.EasingStyle.Sine,
	Enum.EasingDirection.Out,
	0,
	false,
	0
)
local tutinfo = TweenInfo.new(
	.5,
	Enum.EasingStyle.Sine,
	Enum.EasingDirection.Out,
	999999999999999999,
	true,
	.5
)
local tutorialtw = TweenService:Create(TopBar,tutinfo,{CanvasPosition = Vector2.new(100, 0)})
tutorialtw:Play()
for i,v in pairs(TopBar:GetChildren()) do
	v.MouseWheelForward:Connect(function()
		tutorialtw:Pause()
	end)
	v.MouseWheelBackward:Connect(function()
		tutorialtw:Pause()
	end)
end
TopBar.MouseWheelForward:Connect(function()
	tutorialtw:Pause()
end)
TopBar.MouseWheelBackward:Connect(function()
	tutorialtw:Pause()
end)
print("SUCCESSFULLY LOADED OBIOUS BY jaorbw#5640 AND scope#9999")
local function GetPlayer(input) 
	for _, plr in pairs(game.Players:GetPlayers()) do
		if (string.lower(input) == string.sub(string.lower(plr.Name), 1, #input)) then
			return plr;
		elseif (string.lower(input) == string.sub(string.lower(plr.DisplayName), 1, #input)) then
			return plr;
		end
	end
end
for i,button in pairs(TopBar:GetChildren()) do
	if button:IsA("TextButton") then
		button.MouseButton1Click:Connect(function()
			local change = {Position = UDim2.new(button.Position.X.Scale,button.Position.X.Offset, 0.547,0)}
			local ChangePos = TweenService:Create(TopBar.fondo,info,change)
			ChangePos:Play()
			for i, frame in pairs(Background:GetChildren()) do
				if frame:IsA("Frame") then
					if frame.Name ~= "Logo" then 
						frame.Visible = false
					end
					Background:FindFirstChild(button.Name).Visible = true
					if Background:FindFirstChild("Tutorial") then
						Background:FindFirstChild("Tutorial"):Destroy()
					end
				end
			end
		end)
	end
end
